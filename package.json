{
  "name": "jsxhint",
  "version": "0.0.1",
  "description": "Wrapper for JSHint to allow hinting of JSX files",
  "main": "jsxhint.js",
  "bin": "./cli.js",
  "scripts": {
    "test": "./node_modules/.bin/mocha",
    "lint": "./node_modules/.bin/jshint"
  },
  "repository": {
    "type": "git",
    "url": "CondeNast/JSXHint"
  },
  "prefer_global": true,
  "keywords": [
    "jshint",
    "jsx",
    "react",
    "lint",
    "jslint",
    "reactjs"
  ],
  "author": "Todd Kennedy",
  "license": "MIT",
  "dependencies": {
    "runnel": "~0.5.1",
    "glob": "~3.2.6",
    "jshint": "~2.1.10",
    "react-tools": "~0.4.1",
    "argparse": "~0.1.15",
    "colors": "~0.6.2"
  },
  "devDependencies": {
    "mocha": "~1.13.0",
    "chai": "~1.8.0"
  }
}

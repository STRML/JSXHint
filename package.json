{
  "name": "jsxhint",
  "version": "0.4.1",
  "description": "Wrapper for JSHint to allow hinting of JSX files",
  "main": "jsxhint.js",
  "bin": "./cli.js",
  "scripts": {
    "test": "tap test/test.js",
    "lint": "jshint",
    "postinstall": "if [[ -d .git ]]; then cp pre-commit.sh .git/hooks/pre-commit && chmod +x .git/hooks/pre-commit; fi",
    "precommit": "npm run lint && npm test"
  },
  "repository": {
    "type": "git",
    "url": "CondeNast/JSXHint"
  },
  "prefer_global": true,
  "keywords": [
    "jshint",
    "jsx",
    "react",
    "lint",
    "jslint",
    "reactjs"
  ],
  "author": "Todd Kennedy",
  "contributors": ["Samuel Reed <sam@tixelated.com>", "Casey Foster <c@sey.me>"],
  "license": "MIT",
  "dependencies": {
    "jshint": "~2.3.0",
    "react-tools": "~0.9.0",
    "jstransform": "~2.0.1",
    "through": "~2.3.4",
    "async": "~0.2.9",
    "mkdirp": "~0.3.5"
  },
  "devDependencies": {
    "tap": "~0.4.6"
  }
}
